      <bean id="model2" class="com.annotation.Model">
       <property name="id" value="11"/>
       <property name="name" value="tata"/>
       <property name="carmodel"  value="glanza"/>
    </bean>

    <bean id="car" class="com.annotation.Car">
        
    </bean>
     <context:annotation-config />
    

</beans>